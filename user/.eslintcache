t temp = useSelector(state => state.userReducer.local)\n\n  useEffect(() => {\n    setLocal(localStorage.getItem('access_token'))\n  }, [temp])\n\n  useEffect(() => {\n    // console.log('ini wishlist >>>>', wishlists)\n    let isWishlist = false\n    for (let i = 0; i < wishlists.length; i++) {\n      if (wishlists[i].EventId === props.data.id) {\n        isWishlist = true\n      }\n    }\n    if (isWishlist) {\n      setShowButton(false) // remove button\n    } else {\n      setShowButton(true) // add button\n    }\n  }, [wishlists])\n\n  function goDetail(id) {\n    history.push(`/event/${id}`)\n  }\n\n  function handleAddWishlist() {\n    dispatch(addWishlist(props.data.id))\n  }\n\n  function handleRemoveWishlist() {\n    for (let i = 0; i < wishlists.length; i++) {\n      if (wishlists[i].EventId === props.data.id) {\n        dispatch(removeWishlist(wishlists[i].id))\n      }\n    }\n  }\n\n  return (\n      <Col lg={6}>\n        <Card style={{ flexDirection: 'row', margin: 10, width: '600px', height: 145 }}>\n          <Card.Img style={{ width: '125px' }} src={props.data.event_preview} />\n          <Card.Body style={{width: 500}}>\n            <h2><strong>{props.data.title}</strong></h2>\n            <Card.Text>\n              <h4>{props.data.location}</h4>\n            </Card.Text>\n            <Row>\n              <Col sm={6}>\n                <h6>{props.data.date}</h6>\n                <h6>{props.data.time}</h6>\n              </Col>\n            </Row>\n          </Card.Body>\n          <Card.Footer style={{ width: '250px' }}>\n            {local &&\n              (\n                <div>\n                  <Button\n                    onClick={() => goDetail(props.data.id)}\n                    style={{\n                      margin: '28px 0px 0px 0px',\n                      width: '150px',\n                      height: '30px',\n                      color: 'whitesmoke',\n                      backgroundColor: '#F2C94C',\n                      border: 'none',\n                      boxShadow: '0 4px 8px 0 rgba(0,0,0,0.2)'\n                    }}>\n                    Detail\n                  </Button>\n                  { (location.pathname === '/' && showButton) ?\n                    (\n                      <Button\n                        onClick={handleAddWishlist}\n                        style={{\n                          margin: '8px 0px 12px 0px',\n                          width: '150px',\n                          height: '30px',\n                          color: 'whitesmoke',\n                          backgroundColor: '#F2C94C',\n                          border: 'none',\n                          boxShadow: '0 4px 8px 0 rgba(0,0,0,0.2)'\n                        }}>\n                        Add to Wishlist\n                      </Button>\n                    ) :\n                    (\n                      <Button\n                        onClick={handleRemoveWishlist}\n                        style={{\n                          margin: '8px 0px 12px 0px',\n                          width: '150px',\n                          height: '30px',\n                          color: 'whitesmoke',\n                          backgroundColor: '#F2C94C',\n                          border: 'none',\n                          boxShadow: '0 4px 8px 0 rgba(0,0,0,0.2)'\n                        }}>\n                        Remove from Wishlist\n                      </Button>\n                    )}\n                </div>\n              )}\n          </Card.Footer>\n        </Card>\n      </Col>\n  )\n}","/home/aldstn/Documents/p3/final project/client/user/src/hooks/apiRequest.js",[],"/home/aldstn/Documents/p3/final project/client/user/src/component/FooterPage.jsx",["166","167","168"],"import React from \"react\";\n\n// reactstrap components\nimport { Row, Container } from \"reactstrap\";\n\nfunction DemoFooter() {\n  return (\n    <footer className=\"footer footer-black footer-white\">\n      <Container>\n       